@model IEnumerable<Match>


<div id="container">
    <h2 id="conteiner" class="lists">
        @if (Model.Count() > 0)
        {
            <h2>Matches</h2>
            @Html.Partial("MatchesList", Model)

        }
        else
        {
            <h2>
                There are no matches available to buy. Thank you !
            </h2>
        }
    </div>
</div>